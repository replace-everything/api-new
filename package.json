{
  "name": "api",
  "packageManager": "yarn@1.22.1",
  "version": "0.0.2",
  "description": "",
  "private": true,
  "license": "MIT",
  "preserveConstEnums": true,
  "strictNullChecks": true,
  "resolutions": {
    "class-transformer": "0.5.1",
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "scripts": {
    "aws:configure": "aws configure",
    "aw:yeah:dude": "node scripts/interactiveDeploy.mjs",
    "build:mode": "npx webpack --config webpack.config.js --mode",
    "build:dev": "yarn build:mode development",
    "build:deploy": "yarn build:mode production",
    "build:lambda": "yarn format:lint && yarn build:mode production --env entry=photo-processor",
    "deploy:stage": "yarn format:lint && yarn build:mode production && sls deploy --stage",
    "deploy:offline": "yarn build:dev && sls offline --verbose",
    "deploy:offline:prod": "yarn build:deploy && sls offline",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps}/**/*.ts\" --fix",
    "format:lint": "yarn format && yarn lint",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.513.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/static": "^7.0.1",
    "@fastify/view": "^8.2.0",
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.3",
    "@nestjs/mapped-types": "2.0.5",
    "@nestjs/microservices": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/platform-fastify": "^10.3.3",
    "@nestjs/platform-socket.io": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.3",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "aws-serverless-fastify": "3.0.6",
    "busboy": "^1.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "fastify": "^4.26.1",
    "file-type": "^19.0.0",
    "formidable": "3.5.1",
    "ioredis": "^5.3.2",
    "jimp": "^0.22.10",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.3.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.1",
    "nats": "^2.19.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-lambda": "^3.504.0",
    "@aws-sdk/client-s3": "^3.504.0",
    "@aws-sdk/client-secrets-manager": "^3.504.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.1",
    "@types/aws-lambda-fastify": "^1.4.3",
    "@types/dotenv": "^8.2.0",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "node-mocks-http": "^1.14.1",
    "prettier": "^3.0.0",
    "prettier-eslint": "^16.1.2",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-jest-plugin": "^0.4.0",
    "serverless-offline": "^13.3.3",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-warmup": "^8.3.0",
    "terser": "^5.27.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "main": "./src/main.ts",
  "repository": "https://github.com/raesdevs/api.git"
}
